        // User
        h3 User
        table.table.table-striped.table-bordered.table-hover.table-responsive
            thead.thead-dark
                tr
                    th(scope='col') Route
                    th(scope='col') Params
                    th(scope='col') Method
                    th(scope='col') Access
                    th(scope='col') Body
                    th(scope='col') Response success, Status
                    th(scope='col') Response error, Status
                    th(scope='col' style='min-width: 461px !important') Description
            tbody
                tr
                    td /user
                    td
                    td
                        span.badge.badge-success GET
                    td Private
                    td
                    td
                        pre.
                            {
                                _id: string,
                                username: string,
                                email: string
